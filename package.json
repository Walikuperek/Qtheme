{
  "name": "@quak.lib/qtheme",
  "version": "1.5.17",
  "description": "Easy light/dark/infinite. Manage themes in JS/TS/any framework. Production-ready, well documented and tested.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://quak.com.pl/lib/qtheme/index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/Walikuperek/Qtheme.git"
  },
  "bugs": {
    "url": "https://github.com/Walikuperek/Qtheme/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest --config jestconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "publish": "npm run build && npm publish --access public"
  },
  "keywords": [
    "react theme",
    "angular theme",
    "react theming",
    "angular theming",
    "js theme",
    "ts theme",
    "css-in-js",
    "theming",
    "theme",
    "themes",
    "theme changer",
    "theme manager",
    "quak"
  ],
  "author": "QUAK Kacper Walczak",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3"
  }
}
